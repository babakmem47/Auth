@model IEnumerable<Auth.Person>

@{
    ViewBag.Title = "title";
}

<h2>Persons</h2>

<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Semat</th>
        <th>Tel</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Semat.Name</td>
                <td>@item.Tel</td>
            </tr>
        }
    </tbody>
</table>

